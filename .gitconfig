[user]
	name = Michael Griffiths
	email = mikey@cich.li

[credential]
	helper = osxkeychain

[branch]
	autoSetupMerge = always
	autoSetupRebase = always
[remote]
	pushDefault = origin

[core]
	autocrlf = input
	excludesfile = ~/.gitignore_global

[diff]
	algorithm = histogram
[diff "clojure"]
	xfuncname = (^\\(.*|\\s*\\(defn.*)

[rerere]
	enabled = true
